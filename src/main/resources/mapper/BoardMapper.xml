<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.boardver4.Board.BoardMapper">
    <insert id="InsBoard" parameterType="BoardInsDto">
        insert into t_board
        (title,ctnt,iuser)
        values
        (#{title},#{ctnt},#{iuser})

    </insert>
    <select id="selBoard" resultType="BoardSelDto">
        select iboard,title,ctnt,iuser,created_at createdAt
        from t_board
        Limit #{startIdx},#{row}
    </select>
</mapper>